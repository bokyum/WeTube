extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Search by #{searchingBy}
    .search__videos
        if videos.length === 0
            h5 No Videos Found
        each video in videos
            +videoBlock({
                title:video.title,
                view:video.views,
                videoFile:video.fileUrl,
                id:video.id
            })